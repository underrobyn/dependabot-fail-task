{
  "manifestVersion": 1,
  "id": "dependabot-fail-task",
  "publisher": "robyn-zip",
  "version": "1.0.0",
  "name": "Dependabot Fail Task",
  "description": "Fail a pipeline based on dependabot alerts open on a repo",
  "public": false,
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "images/dependabot-fail-task.png"
  },
  "tags": [
    "dependabot",
    "build",
    "fail",
    "github"
  ],
  "scopes": [
    "vso.serviceendpoint"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "contributions": [
    {
      "id": "dependabot-fail-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "task"
      }
    }
  ],
  "files": [
    {
      "path": "task"
    },
    {
      "path": "images",
      "addressable": true
    }
  ]
}